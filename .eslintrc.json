{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
      "no-underscore-dangle": "off",
      "linebreak-style": "off",
      "no-undef": "off",
      "class-methods-use-this": 0,
      "max-len": ["error", {"code": 140}]
    }
}
